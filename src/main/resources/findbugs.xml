<?xml version="1.0" encoding="UTF-8"?>
<FindbugsPlugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="findbugsplugin.xsd"
        pluginid="edu.nyu.websocket-spotbugs">

        <Detector class="edu.nyu.MyDetector" reports="MY_BUG" />
        <Detector class="edu.nyu.websocket.WebSocketDetector" reports="WEBSOCKET_BUG" />

        <BugPattern abbrev="MY" type="MY_BUG" category="CORRECTNESS" />
        <BugPattern abbrev="WS" type="WEBSOCKET_BUG" category="CORRECTNESS" />
</FindbugsPlugin>
